<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	
    <package name="default" namespace="/" extends="struts-default">
       	<action name="login" class="com.studentyears.Login.LoginAction" method="execute">
            <result name="success">/welcome.jsp</result>
            <result name="login">/test.jsp</result>
        </action>
        <action name="login1" class="com.studentyears.Login.ListAction" method="execute">
            <result name="success">/showlist.jsp</result>
            <result name="login">/test.jsp</result>
        </action>
        <action name="herf" class="com.studentyears.Login.HerfAction" method="execute">
            <result name="success">/herf.jsp</result>
            <result name="login">/test.jsp</result>
        </action>
    </package>
    
    <package name="struts2" extends="struts-default">
        <action name="upload" class="com.studentyears.network.UploadAction" method="execute">  
            <result name="success">/index.jsp</result>  
            <result name="input">/upload.jsp</result>  
        </action>  
    </package>

</struts>
